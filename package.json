{
    "name": "onur-js",
    "module": "src/index.ts",
    "type": "module",
    "version": "0.2.0",
    "scripts": {
        "tests": "bun test",
        "eslint": "eslint",
        "lint": "eslint --fix --ext .ts .",
        "format": "prettier --write ./src",
        "format-check": "prettier --check ./src",
        "watch": "bun run --watch src/index.ts -- --grab",
        "build": "bun build ./src/index.ts --compile --outfile onur",
        "grab": "bun run src/index.ts -- --grab",
        "backup": "bun run src/index.ts -- --backup meh,foo,bar",
        "usage": "bun run src/index.ts -- --help"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "bun-types": "latest",
        "eslint": "^8.57.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.6.0",
        "prettier": "3.2.1",
        "typescript": "^5.5.4"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    }
}
